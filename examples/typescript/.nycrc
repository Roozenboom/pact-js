{
  "include": [
    "src/*.ts",
    "src/**/*.ts"
  ],
  "exclude": [
    "**/*.d.ts",
    "**/*.spec.ts"
  ],
  "extension": [
    ".ts"
  ],
  "require": [
    "ts-node/register"
  ],
  "reporter": [
    "text-summary",
    "lcovonly"
  ],
  "sourceMap": true,
  "instrument": true,
  "lines": 50,
  "statements": 50,
  "functions": 50,
  "branches": 50
}
